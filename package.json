  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evwhite2/react-portfolio/issues"
  },
  "homepage": "https://github.com/evwhite2/react-portfolio#readme",
  "dependencies": {
    "concurrently": "^5.1.0",
    "mongoose": "^5.9.2",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "express": "^4.17.1",
    "if-env": "^1.0.4"
  }
}
